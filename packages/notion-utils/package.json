{
  "name": "notion-utils",
  "version": "6.16.1",
  "type": "module",
  "description": "Useful utilities for working with Notion data. Isomorphic.",
  "repository": "NotionX/react-notion-x",
  "author": "Travis Fischer <travis@transitivebullsh.it>",
  "license": "MIT",
  "main": "./build/index.js",
  "module": "./build/index.js",
  "types": "./build/index.d.ts",
  "exports": {
    ".": {
      "types": "./build/index.d.ts",
      "default": "./build/index.js"
    }
  },
  "sideEffects": false,
  "files": [
    "build"
  ],
  "engines": {
    "node": ">=12"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf build",
    "dev": "tsup --watch",
    "lint": "eslint '**/*.{ts,tsx}'",
    "publish": "pnpm publish",
    "test": "ava"
  },
  "dependencies": {
    "is-url-superb": "^6.1.0",
    "mem": "^9.0.2",
    "normalize-url": "^7.0.3",
    "notion-types": "workspace:*",
    "p-queue": "^7.2.0"
  },
  "ava": {
    "snapshotDir": ".snapshots",
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--import=tsimp"
    ]
  },
  "devDependencies": {
    "@ava/typescript": "^5.0.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.6.3"
  }
}
